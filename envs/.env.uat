# UAT Environment Variables

NODE_ENV=uat
PORT=3000
API_PREFIX=api
CORS_ORIGIN=https://uat.yourdomain.com

# Database Configuration
DB_HOST=uat-db-host
DB_PORT=3306
DB_USERNAME=uat_user
DB_PASSWORD=uat_password
DB_DATABASE=wms_uat
DB_SYNCHRONIZE=false
DB_LOGGING=false

# JWT Configuration
JWT_SECRET=your-uat-secret-key-change-me
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=your-uat-refresh-secret-key
JWT_REFRESH_EXPIRES_IN=30d
